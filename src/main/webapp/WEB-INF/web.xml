<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  
  <servlet>
  
  <servlet-name>sign</servlet-name>
  <servlet-class>StudentWithHtml.SignupServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  
  <servlet-name>sign</servlet-name>
  <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
  <servlet>
  
  <servlet-name>lo</servlet-name>
  <servlet-class>StudentWithHtml.LoginServlet</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  <servlet-name>lo</servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>sel</servlet-name>
  <servlet-class>StudentWithHtml.SelectServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>sel</servlet-name>
  <url-pattern>/select</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name>de</servlet-name>
  <servlet-class>StudentWithHtml.DeleteServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>de</servlet-name>
  <url-pattern>/delete</url-pattern>
  </servlet-mapping>

<servlet>
<servlet-name>up</servlet-name>
<servlet-class>StudentWithHtml.UpdateServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>up</servlet-name>
<url-pattern>/update</url-pattern>
</servlet-mapping>
</web-app>
